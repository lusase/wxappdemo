.container{
    display: flex;
    flex-direction: column;
    align-items: center;
    background-color: #b3d4db;
}
.user-name{
    margin-top: 100rpx;
    font-size: 32rpx;
    font-weight: 700;
}
.user-avatar{
    display: block;
    width:200rpx;
    height: 200rpx;
    margin:160rpx auto 0;
    border-radius: 50%;
}
.moto-wrapper{
    margin-top: 200rpx;
    border: 1rpx solid #405f80;
    width: 200rpx;
    height: 80rpx;
    border-radius: 5rpx;
    text-align: center;
}
.moto{
    font-size: 22rpx;
    font-weight: 700;
    line-height: 80rpx;
    color: #405f80;
}
page{
    background-color: #b3d4db;
}